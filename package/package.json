{
  "name": "tings",
  "version": "5.0.1",
  "private": false,
  "description": "A collection of small JavaScript utilities",
  "homepage": "https://tings.wstone.uk/",
  "bugs": {
    "url": "https://github.com/will-stone/tings/issues"
  },
  "repository": {
    "url": "https://github.com/will-stone/tings"
  },
  "license": "MIT",
  "author": {
    "name": "Will Stone",
    "email": "hello@wstone.uk",
    "url": "https://wstone.uk"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "postbuild": "rimraf ../website/data/typedoc.json && typedoc",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typedoc": "^0.23.20",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.0",
    "vitest": "^0.25.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
